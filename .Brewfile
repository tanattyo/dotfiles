# Brew bundle command list
# see https://github.com/Homebrew/homebrew-bundle
# brew bundle: Brewfileから一括ダウンロード
# brew bundle dump: ダウンロードリストをダンプ
# brew bundle cleanup: リストにないものを削除する
# brew bundle check: リストにあるもののうちアップグレードが必要なものをリストアップ
# brew bundle list: リストを一覧表示する

# Brewfile
cask_args appdir: "/Applications"
tap "homebrew/bundle"
tap "homebrew/core"

brew "curl"
brew "docker", link: false
brew "gist"
brew "git"
brew "go"
brew "imagemagick"
brew "nodebrew"
brew "openssl"
brew "peco"
brew "shellcheck"
brew "p7zip"
brew "tmux"
brew "terraformer"
brew "tfenv"
brew "tree"
brew "vim"
brew "wget"
brew "zsh"
brew "zsh-completions"

# cask
cask "alfred"
cask "appcleaner"
cask "atom"
cask "caffeine"
cask "coteditor"
cask "docker"
cask "discord"
cask "dropbox"
cask "evernote"
cask "gimp"
cask "google-chrome"
cask "google-japanese-ime"
cask "github"
cask "gyazo"
cask "hyper"
cask "hyperswitch"
cask "iterm2"
cask "karabiner-elements"
cask "keycastr"
cask "kindle"
cask "qblocker"
cask "scroll-reverser"
cask "spectacle"
cask "slack"
cask "visual-studio-code"
cask "zoom"
